<li><span class="ib-brac">(</span><span class="ib-content"><a href="http://ejje.weblio.jp/content/astrology" title="astrologyの意味" class=crosslink>astrology</a></span><span class="ib-brac">)</span> <a href="http://ejje.weblio.jp/content/An" title="Anの意味" class=crosslink>An</a> <a href="http://ejje.weblio.jp/content/essential" title="essentialの意味" class=crosslink>essential</a> <a href="http://ejje.weblio.jp/content/dignity" title="dignityの意味" class=crosslink>dignity</a> <a href="http://ejje.weblio.jp/content/in+which" title="in whichの意味" class=crosslink>in which</a> <a href="http://ejje.weblio.jp/content/unequal" title="unequalの意味" class=crosslink>unequal</a> <a href="http://ejje.weblio.jp/content/segments" title="segmentsの意味" class=crosslink>segments</a> <a href="http://ejje.weblio.jp/content/of" title="ofの意味" class=crosslink>of</a> every <a href="http://ejje.weblio.jp/content/astrological" title="astrologicalの意味" class=crosslink>astrological</a> <a href="http://ejje.weblio.jp/content/sign" title="signの意味" class=crosslink>sign</a> have <a href="http://ejje.weblio.jp/content/internal" title="internalの意味" class=crosslink>internal</a> <a href="http://ejje.weblio.jp/content/rulerships" title="rulershipsの意味" class=crosslink>rulerships</a> which <a href="http://ejje.weblio.jp/content/affect" title="affectの意味" class=crosslink>affect</a> the <a href="http://ejje.weblio.jp/content/power" title="powerの意味" class=crosslink>power</a> and <a href="http://ejje.weblio.jp/content/integrity" title="integrityの意味" class=crosslink>integrity</a> <a href="http://ejje.weblio.jp/content/of" title="ofの意味" class=crosslink>of</a> each <a href="http://ejje.weblio.jp/content/planet" title="planetの意味" class=crosslink>planet</a> <a href="http://ejje.weblio.jp/content/in+a" title="in aの意味" class=crosslink>in a</a> <a href="http://ejje.weblio.jp/content/natal" title="natalの意味" class=crosslink>natal</a> chart.</li>
